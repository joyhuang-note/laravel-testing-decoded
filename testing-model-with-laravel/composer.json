{
	"require": {
		"laravel/framework": "4.2.*"
	},
    "require-dev": {
        "phpunit/phpunit": "5.1.0",
        "mockery/mockery": "0.9.4",
        "way/laravel-test-helpers": "dev-master",
        "doctrine/dbal": "2.5.4"
    },
	"autoload": {
		"classmap": [
			"app/commands",
			"app/controllers",
			"app/models",
			"app/database/migrations",
			"app/database/seeds",
			"app/tests/TestCase.php",
			"app/tests/helpers"
		]
	},
	"minimum-stability": "stable"
}
